{
    "dotnet.preferCSharpExtension": true,
    "dotnet.defaultSolution": "CleanFormerlySerializedAs.sln",
    "omnisharp.useModernNet": true,
    "omnisharp.enableRoslynAnalyzers": true,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "terminal.integrated.cwd": "${workspaceFolder}",
    "files.exclude": {
        ".plastic": true,
        ".idea": true,
        ".cursorignore": true,
        ".cursor": true,
        ".vscode": true,
        "**/*.meta": true,
        "**/Library": true,
        "**/Temp": true,
        "**/obj": true,
        "**/Build": true,
        "**/Logs": true,
        "**/*.DotSettings*": true,
        "ProjectSettings": true
    },
    "search.exclude": {
        ".plastic": true,
        ".idea": true,
        ".cursorignore": true,
        ".cursor": true,
        ".vscode": true,
        "**/*.meta": true,
        "**/Library": true,
        "**/Temp": true,
        "**/obj": true,
        "**/Build": true,
        "**/Logs": true,
        "**/*.csproj": true,
        "**/*.sln": true,
        "**/*.DotSettings*": true,
        "ProjectSettings": true
    }
}